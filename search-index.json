ller","u":"/getting-started/installation","h":"#install-using-web-installer","p":1},{"i":9,"t":"Variables","u":"/syntax/template","h":"#variables","p":7},{"i":11,"t":"Structures","u":"/syntax/template","h":"#structures","p":7},{"i":17,"t":"Basic","u":"/syntax/types/multiselect","h":"#basic","p":15},{"i":19,"t":"Custom values","u":"/syntax/types/multiselect","h":"#custom-values","p":15},{"i":21,"t":"Iterate over results","u":"/syntax/types/multiselect","h":"#iterate-over-results","p":15},{"i":25,"t":"Basic","u":"/syntax/types/number","h":"#basic","p":23},{"i":27,"t":"Validation","u":"/syntax/types/number","h":"#validation","p":23},{"i":28,"t":"Minimum","u":"/syntax/types/number","h":"#minimum","p":23},{"i":30,"t":"Maximum","u":"/syntax/types/number","h":"#maximum","p":23},{"i":34,"t":"Basic","u":"/syntax/types/boolean","h":"#basic","p":32},{"i":38,"t":"Basic","u":"/syntax/types/text","h":"#basic","p":36},{"i":40,"t":"Multiline","u":"/syntax/types/text","h":"#multiline","p":36},{"i":42,"t":"Validation","u":"/syntax/types/text","h":"#validation","p":36},{"i":43,"t":"Regex","u":"/syntax/types/text","h":"#regex","p":36},{"i":48,"t":"Basic","u":"/syntax/types/select","h":"#basic","p":46},{"i":50,"t":"Custom values","u":"/syntax/types/select","h":"#custom-values","p":46}],"index":{"version":"2.3.9","fields":["t"],"fieldVectors":[["t/3",[0,1.439,1,1.439,2,1.802]],["t/5",[0,1.899,1,1.206,3,1.51]],["t/9",[4,2.94]],["t/11",[5,2.94]],["t/17",[6,1.435]],["t/19",[7,1.785,8,1.785]],["t/21",[9,1.802,10,1.802,11,1.802]],["t/25",[6,1.435]],["t/27",[12,2.348]],["t/28",[13,2.94]],["t/30",[14,2.94]],["t/34",[6,1.435]],["t/38",[6,1.435]],["t/40",[15,2.94]],["t/42",[12,2.348]],["t/43",[16,2.94]],["t/48",[6,1.435]],["t/50",[7,1.785,8,1.785]]],"invertedIndex":[["basic",{"_index":6,"t":{"17":{"position":[[0,5]]},"25":{"position":[[0,5]]},"34":{"position":[[0,5]]},"38":{"position":[[0,5]]},"48":{"position":[[0,5]]}}}],["custom",{"_index":7,"t":{"19":{"position":[[0,6]]},"50":{"position":[[0,6